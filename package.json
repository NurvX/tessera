{
  "name": "ticketshop",
  "version": "0.1.0",
  "private": false,
  "description": "Full-stack ticket shop with admin dashboard.",
  "author": "Marius Butz",
  "homepage": "https://github.com/mbpictures/ticketshop#readme",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:test": "cross-env EMAIL_PROVIDER= EMAIL_HOST=localhost EMAIL_PORT=7777 next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "start:default": "next start -p 3000",
    "start:test": "cross-env EMAIL_PROVIDER= EMAIL_HOST=localhost EMAIL_PORT=7777 next start -p 3000",
    "vercel-build": "prisma generate && prisma migrate reset --force --skip-seed && prisma db push && prisma db seed && next build",
    "heroku-postbuild": "prisma generate && prisma migrate reset --force --skip-seed && prisma db push && prisma db seed && next build",
    "prisma:generate": "prisma generate",
    "ngrok": "ngrok http 3000",
    "lint": "next lint",
    "analyze": "set ANALYZE=true && npm run build",
    "cypress:build": "prisma generate && prisma migrate reset --force --skip-seed && prisma db push && cross-env INSTRUMENT_CODE=true next build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run-ct": "cypress run --component --env INSTRUMENT_CODE=true",
    "cypress:cov": "npx nyc report --reporter=text-summary"
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "pre-commit": [
    "lint"
  ],
  "pkg": {
    "assets": [
      "src/assets/**/*",
      "public/**/*"
    ]
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "nyc": {
    "all": true,
    "include": "src/**"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.7",
    "@mui/icons-material": "^5.8.3",
    "@mui/lab": "^5.0.0-alpha.85",
    "@mui/material": "^5.8.3",
    "@mui/styles": "^5.8.3",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/react-paypal-js": "^7.8.1",
    "@prisma/client": "^3.15.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@stripe/react-stripe-js": "^1.8.1",
    "@stripe/stripe-js": "^1.31.0",
    "ajv": "^8.11.0",
    "apexcharts": "^3.35.3",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "copy-webpack-plugin": "^11.0.0",
    "country-region-data": "^1.11.0",
    "currency-symbol-map": "^5.1.0",
    "date-fns": "^2.28.0",
    "ejs": "^3.1.8",
    "fast-xml-parser": "^4.0.8",
    "formidable": "^2.0.1",
    "formik": "^2.2.9",
    "framer-motion": "^6.3.11",
    "html-pdf": "^3.0.1",
    "json-2-csv": "^3.17.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "mui-color": "^2.0.0-beta.2",
    "next": "^12.2.0",
    "next-absolute-url": "^1.2.2",
    "next-auth": "^4.4.0",
    "next-translate": "^1.4.0",
    "nodemailer": "^6.7.5",
    "notistack": "^2.0.5",
    "paypal-checkout": "^4.0.336",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.0",
    "raw-body": "^2.5.1",
    "react": "17.0.2",
    "react-apexcharts": "^1.4.0",
    "react-dom": "17.0.2",
    "react-flag-kit": "^1.1.1",
    "react-json-view": "^1.21.3",
    "react-json-viewer": "^3.0.1",
    "react-parallax-tilt": "^1.7.40",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-redux": "^8.0.2",
    "react-zoom-pan-pinch": "^2.1.3",
    "request-ip": "^2.2.0",
    "sass": "^1.52.3",
    "simplebar-react": "^2.4.1",
    "stripe": "^9.8.0",
    "uuid": "^8.3.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webrtc-adapter": "^8.1.1",
    "yup": "^0.32.11",
    "zippo": "^1.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/react": "^5.12.5",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@types/node": "^17.0.41",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-redux": "^7.1.24",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^10.7.0",
    "cypress-real-events": "^1.7.1",
    "eslint": "8.17.0",
    "eslint-config-next": "^12.2.0",
    "html-webpack-plugin": "^5.5.0",
    "ngrok": "^4.3.1",
    "postcss": "^8.4.14",
    "pre-commit": "^1.2.2",
    "prettier": "2.6.2",
    "prisma": "^3.15.1",
    "smtp-tester": "^1.2.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-dev-server": "^4.9.2"
  }
}
