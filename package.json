{
  "name": "ticketshop",
  "version": "0.1.0",
  "private": false,
  "description": "Full-stack ticket shop with admin dashboard.",
  "author": "Marius Butz",
  "homepage": "https://github.com/mbpictures/ticketshop#readme",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "vercel-build": "prisma generate && prisma migrate reset --force --skip-seed && prisma db push && prisma db seed && next build",
    "heroku-postbuild": "prisma generate && prisma migrate reset --force --skip-seed && prisma db push && prisma db seed && next build",
    "prisma:generate": "prisma generate",
    "ngrok": "ngrok http 3000",
    "lint": "next lint",
    "analyze": "set ANALYZE=true && npm run build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:cov": "npx nyc report --reporter=text-summary"
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "pre-commit": [
    "lint"
  ],
  "pkg": {
    "assets": [
      "src/assets/**/*",
      "public/**/*"
    ]
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "nyc": {
    "all": true,
    "include": "src/**"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.55",
    "@mui/material": "^5.0.2",
    "@mui/styles": "^5.2.3",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/react-paypal-js": "^7.5.0",
    "@prisma/client": "^3.5.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.21.1",
    "ajv": "^8.8.2",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "copy-webpack-plugin": "^10.0.0",
    "country-region-data": "^1.11.0",
    "currency-symbol-map": "^5.0.1",
    "date-fns": "^2.28.0",
    "ejs": "^3.1.6",
    "fast-xml-parser": "^4.0.0-beta.2",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "html-pdf": "^3.0.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "mui-color": "^2.0.0-beta.1",
    "next": "^12.0.6",
    "next-absolute-url": "^1.2.2",
    "next-auth": "^4.0.6",
    "nodemailer": "^6.7.2",
    "notistack": "^2.0.3",
    "paypal-checkout": "^4.0.331",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-json-view": "^1.21.3",
    "react-json-viewer": "^3.0.1",
    "react-redux": "^7.2.5",
    "react-zoom-pan-pinch": "^2.1.3",
    "request-ip": "^2.1.3",
    "sass": "^1.42.1",
    "simplebar-react": "^2.3.6",
    "stripe": "^8.191.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "yup": "^0.32.11",
    "zippo": "^1.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/react": "^5.12.1",
    "@types/node": "^16.11.9",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-redux": "^7.1.19",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^9.3.1",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "ngrok": "^4.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "2.5.1",
    "prisma": "^3.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
